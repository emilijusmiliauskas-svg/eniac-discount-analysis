      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "merged[\"sub_category\"] = merged[\"type\"].map(sub_map)\n",
        "merged[\"category\"] = merged[\"type\"].map(cat_map)"
      ],
      "metadata": {
        "id": "cMneib_WvTEe"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "merged[\"sub_category\"] = merged[\"type\"].map(sub_map).fillna(\"Other\")\n",
        "merged[\"category\"] = merged[\"type\"].map(cat_map).fillna(\"Other\")"
      ],
