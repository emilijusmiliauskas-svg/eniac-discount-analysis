    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "bf[\"price\"] = pd.to_numeric(bf[\"price\"], errors=\"coerce\")\n",
        "bf[\"discount%\"] = pd.to_numeric(bf[\"discount%\"], errors=\"coerce\")\n",
        "bf[\"estimated_revenue_per_product\"] = pd.to_numeric(bf[\"estimated_revenue_per_product\"], errors=\"coerce\")\n",
        "\n",
        "# 10 price buckets\n",
        "bf[\"price_bin\"] = pd.cut(bf[\"price\"], bins=10)\n",
        "\n",
        "# aggregates\n",
        "erpp_price = bf.groupby(\"price_bin\")[\"estimated_revenue_per_product\"].sum()\n",
        "avg_disc_price = bf.groupby(\"price_bin\")[\"discount%\"].mean()\n",
        "\n",
        "# plot bars\n",
        "fig, ax = plt.subplots(figsize=(10,5))\n",
        "bars = ax.bar(erpp_price.index.astype(str), erpp_price.values, color=\"tab:purple\")\n",
        "\n",
        "ax.set_xlabel(\"Price range (€)\")\n",
        "ax.set_ylabel(\"ERPP sum\")\n",
        "ax.set_title(\"ERPP sum by price range with avg discount% (BF window)\")\n",
        "\n",
        "# add ERPP labels and avg discount% inside bars\n",
        "for i, bar in enumerate(bars):\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\", color=\"black\")\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h/2, f\"{avg_disc_price.iloc[i]:.1f}%\",\n",
        "            ha=\"center\", va=\"center\", color=\"white\", fontsize=9, fontweight=\"bold\")\n",
        "\n",
        "plt.xticks(rotation=45, ha=\"right\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
