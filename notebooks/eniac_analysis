    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "\n",
        "# define 5 price buckets\n",
        "merged[\"price_range\"] = pd.qcut(merged[\"price\"], q=5)\n",
        "\n",
        "# average discount% per bucket\n",
        "avg_discount = merged.groupby(\"price_range\")[\"discount%\"].mean()\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(8,5))\n",
        "bars = ax.bar(avg_discount.index.astype(str), avg_discount.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Price range\")\n",
        "ax.set_ylabel(\"Average discount %\")\n",
        "ax.set_title(\"Average discount% by price range\")\n",
        "\n",
        "# add labels\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:.1f}%\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.xticks(rotation=45)\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
