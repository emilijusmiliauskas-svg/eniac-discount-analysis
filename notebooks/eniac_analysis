    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(merged[\"estimated_revenue_per_product\"], errors=\"coerce\")\n",
        "\n",
        "# positives only (keep ALL products; no single-order mask)\n",
        "pos = merged[merged[\"discount%\"] > 0].copy()\n",
        "\n",
        "# buckets\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "pos[\"disc_range\"] = pd.cut(pos[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "# aggregates\n",
        "counts = pos.groupby(\"disc_range\").size().reindex(labels, fill_value=0)\n",
        "erpp_sum = pos.groupby(\"disc_range\")[\"estimated_revenue_per_product\"].sum().reindex(labels, fill_value=0.0)\n",
        "\n",
        "# plot: dual-axis grouped bars\n",
        "x = range(len(labels)); width = 0.4\n",
        "fig, ax1 = plt.subplots(figsize=(8,4))\n",
        "\n",
        "b1 = ax1.bar([i - width/2 for i in x], counts.values, width=width, color=\"tab:blue\", label=\"Count\")\n",
        "ax1.set_xticks(list(x), labels)\n",
        "ax1.set_ylabel(\"Count\", color=\"tab:blue\"); ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "\n",
        "ax2 = ax1.twinx()\n",
        "b2 = ax2.bar([i + width/2 for i in x], erpp_sum.values, width=width, color=\"tab:orange\", label=\"ERPP sum\")\n",
        "ax2.set_ylabel(\"ERPP sum\", color=\"tab:orange\"); ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "# labels\n",
        "for bar in b1:\n",
        "    h = bar.get_height()\n",
        "    ax1.text(bar.get_x()+bar.get_width()/2, h, f\"{h:,}\", ha=\"center\", va=\"bottom\", color=\"tab:blue\")\n",
        "for bar in b2:\n",
        "    h = bar.get_height()\n",
        "    ax2.text(bar.get_x()+bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\", color=\"tab:orange\")\n",
        "\n",
        "# legend\n",
        "h1,l1 = ax1.get_legend_handles_labels(); h2,l2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(h1+h2, l1+l2, loc=\"upper left\")\n",
        "\n",
        "plt.title(\"Positive discount buckets: count vs ERPP sum (all products)\")\n",
        "plt.tight_layout(); plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 495
        },
        "id": "aS9QdNXxW7EZ",
