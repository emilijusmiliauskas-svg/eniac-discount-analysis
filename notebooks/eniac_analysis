      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "\n",
        "# fixed bins up to 1800\n",
        "bins = [0, 200, 500, 1000, 1500, 1800]\n",
        "labels = [\"<200\", \"200–500\", \"500–1000\", \"1000–1500\", \"1500–1800\"]\n",
        "\n",
        "merged[\"price_range\"] = pd.cut(merged[\"price\"], bins=bins, labels=labels, right=False)\n",
        "\n",
        "# average discount% per price bucket\n",
        "avg_discount = merged.groupby(\"price_range\")[\"discount%\"].mean()\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(8,5))\n",
        "bars = ax.bar(avg_discount.index.astype(str), avg_discount.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Price range (€)\")\n",
        "ax.set_ylabel(\"Average discount %\")\n",
        "ax.set_title(\"Average discount% by fixed price range\")\n",
        "\n",
        "# add labels on top\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
