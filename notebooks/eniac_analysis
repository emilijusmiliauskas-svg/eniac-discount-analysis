    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# filter only positive discounts\n",
        "pos = merged[merged[\"discount%\"] > 0].copy()\n",
        "\n",
        "# define bins and labels\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "\n",
        "# cut into ranges\n",
        "pos[\"disc_range\"] = pd.cut(pos[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "# count per range\n",
        "counts = pos[\"disc_range\"].value_counts().reindex(labels)\n",
        "\n",
        "# bar chart with blue, thinner bars\n",
        "fig, ax = plt.subplots(figsize=(6,4))\n",
        "bars = ax.bar(counts.index, counts.values, color=\"tab:blue\", width=0.6)  # thinner bars\n",
        "\n",
        "ax.set_ylabel(\"Number of products bought\")\n",
        "ax.set_title(\"Positive discount size distribution\")\n",
        "\n",
        "# add labels\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,}\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
