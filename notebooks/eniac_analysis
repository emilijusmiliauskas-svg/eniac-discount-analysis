        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure discount% is numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.hist(\n",
        "    merged[\"discount%\"],\n",
        "    bins=20,                # 20 buckets\n",
        "    color=\"tab:blue\",\n",
        "    edgecolor=\"black\"\n",
        ")\n",
        "\n",
        "plt.xlabel(\"Discount %\")\n",
        "plt.ylabel(\"Number of SKUs sold\")\n",
        "plt.title(\"Distribution of discount% (20 buckets)\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 507
