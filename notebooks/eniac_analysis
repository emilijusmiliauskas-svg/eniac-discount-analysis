    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# negative discount counts per month\n",
        "neg = merged[merged[\"discount\"] < 0]\n",
        "counts = neg.groupby(neg[\"created_date\"].dt.to_period(\"M\")).size()\n",
        "\n",
        "# total paid sum per month (all rows)\n",
        "paid_sum = merged.groupby(merged[\"created_date\"].dt.to_period(\"M\"))[\"total_paid\"].sum()\n",
        "\n",
        "# align indexes\n",
        "counts.index = counts.index.to_timestamp()\n",
        "paid_sum.index = paid_sum.index.to_timestamp()\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(10,4))\n",
        "\n",
        "# first axis: counts\n",
        "ax1.plot(counts.index, counts.values, color=\"tab:blue\", label=\"Negative discount count\")\n",
        "ax1.set_xlabel(\"Month\")\n",
        "ax1.set_ylabel(\"Negative discount count\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "\n",
        "# second axis: total paid\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(paid_sum.index, paid_sum.values, color=\"tab:orange\", label=\"Total paid sum\")\n",
        "ax2.set_ylabel(\"Total paid sum\", color=\"tab:orange\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "fig.tight_layout()\n",
        "plt.title(\"Negative discounts vs total paid per month\")\n",
        "plt.show()"
      ],
