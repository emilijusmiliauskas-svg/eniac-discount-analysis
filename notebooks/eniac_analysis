    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "\n",
        "# average discount% per category\n",
        "avg_disc_cat = merged.groupby(\"category\")[\"discount%\"].mean().sort_values(ascending=False)\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(10,5))\n",
        "bars = ax.bar(avg_disc_cat.index.astype(str), avg_disc_cat.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Category\")\n",
        "ax.set_ylabel(\"Average discount %\")\n",
        "ax.set_title(\"Average discount% by category\")\n",
        "\n",
        "# add labels on top\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:.1f}%\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.xticks(rotation=45, ha=\"right\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 507
        },
        "id": "kc1CGc-CJdse",
        "outputId": "43dcc75c-98c5-46b1-8c02-766d38343b0b"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 1000x500 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "merged[\"_base_line\"] = merged[\"product_quantity\"] * merged[\"unit_price\"]\n",
        "\n",
        "# group by order\n",
        "g = merged.groupby(\"order_id\")\n",
        "order_total = g[\"total_paid\"].transform(\"first\")       # total order paid\n",
        "base_sum    = g[\"_base_line\"].transform(\"sum\")         # Σ(qty * unit_price) per order\n",
        "qty_sum     = g[\"product_quantity\"].transform(\"sum\")   # total quantity in the order\n",
        "\n",
        "# residual spread per unit\n",
        "residual = order_total - base_sum\n",
        "\n",
        "# estimated revenue per product\n",
        "merged[\"estimated_revenue_per_product\"] = merged[\"unit_price\"] + (residual / qty_sum)\n",
        "\n",
        "# cleanup\n",
        "merged.drop(columns=[\"_base_line\"], inplace=True)"
      ],
      "metadata": {
        "id": "4ALBDuJiOLqf"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "url = \"https://drive.google.com/file/d/1m1ThDDIYRTTii-rqM5SEQjJ8McidJskD/view?usp=share_link\"\n",
        "path = \"https://drive.google.com/uc?export=download&id=\"+url.split(\"/\")[-2]\n",
        "brands_cl = pd.read_csv(path)"
      ],
      "metadata": {
        "id": "-x0HU0F9OMnp"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "merged[\"short\"] = merged[\"sku\"].astype(str).str[:3]"
      ],
      "metadata": {
        "id": "q9Q_3IAvTsQM"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# merge on 'short', keep all rows from merged\n",
        "merged = pd.merge(\n",
        "    merged,\n",
        "    brands_cl,\n",
        "    on=\"short\",\n",
        "    how=\"left\",\n",
        "    suffixes=(\"\", \"_brand\")\n",
        ")\n",
        "\n",
        "# drop duplicate 'short' columns if they exist\n",
        "if \"short\" in merged.columns:\n",
        "    merged = merged.drop(columns=[\"short\"])\n",
        "if \"short_brand\" in merged.columns:\n",
        "    merged = merged.drop(columns=[\"short_brand\"])"
      ],
      "metadata": {
