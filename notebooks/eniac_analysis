        "# keep only one row per order_id\n",
        "order_totals = merged.groupby(\"order_id\")[\"total_paid\"].first()\n",
        "\n",
        "# total revenue\n",
        "total_revenue = order_totals.sum()\n",
        "\n",
        "print(\"Total revenue:\", total_revenue)"
      ],
