      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged.get(\"created_date\", merged[\"date\"]), errors=\"coerce\")\n",
        "\n",
        "# per-order revenue (one total_paid per order)\n",
        "order_rev = (\n",
        "    merged.sort_values(\"created_date\")\n",
        "          .groupby(\"order_id\", as_index=False)\n",
        "          .agg(total_paid=(\"total_paid\",\"first\"), date=(\"created_date\",\"first\"))\n",
        ")\n",
        "\n",
        "# 11-day window\n",
        "start = pd.Timestamp(\"2017-11-19\")\n",
        "end   = pd.Timestamp(\"2017-11-29\")\n",
        "\n",
        "mask = (order_rev[\"date\"] >= start) & (order_rev[\"date\"] <= end)\n",
        "\n",
        "bf11_sum = order_rev.loc[mask, \"total_paid\"].sum()\n",
        "all_sum  = order_rev[\"total_paid\"].sum()\n",
        "prop = bf11_sum / all_sum if all_sum else float(\"nan\")\n",
        "\n",
        "print(f\"Black Friday window revenue (2017-11-19 to 2017-11-29): {bf11_sum:,.2f}\")\n",
        "print(f\"Total revenue (all data): {all_sum:,.2f}\")\n",
        "print(f\"Proportion of total: {prop:.2%}\")"
      ],
      "metadata": {
