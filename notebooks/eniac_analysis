      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "# count unique orders per sku\n",
        "order_counts = merged.groupby(\"sku\")[\"order_id\"].nunique()\n",
        "\n",
        "# keep only skus with >1 order\n",
        "multi_order_skus = order_counts[order_counts > 1].index\n",
        "\n",
        "# compute std of unit_price for those skus\n",
        "std_prices = (\n",
        "    merged[merged[\"sku\"].isin(multi_order_skus)]\n",
        "    .groupby(\"sku\")[\"unit_price\"]\n",
        "    .std()\n",
        ")\n",
        "std_prices.describe()"
