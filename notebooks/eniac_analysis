      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime once\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# filter negative discounts\n",
        "neg = merged[merged[\"discount\"] < 0]\n",
        "\n",
        "# group by month\n",
        "counts = neg.groupby(neg[\"created_date\"].dt.to_period(\"M\")).size()\n",
        "print(counts)\n",
        "\n",
        "# plot\n",
        "counts.plot(kind=\"line\", figsize=(10,4))\n",
        "plt.title(\"Negative discount counts per month\")\n",
        "plt.xlabel(\"Month\")\n",
        "plt.ylabel(\"Count\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
