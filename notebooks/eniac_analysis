    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "\n",
        "# positives only\n",
        "pos = merged[merged[\"discount%\"] > 0].copy()\n",
        "\n",
        "# discount% buckets\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "pos[\"disc_range\"] = pd.cut(pos[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "# average price per range\n",
        "avg_price = pos.groupby(\"disc_range\")[\"price\"].mean().reindex(labels, fill_value=0.0)\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(7,4))\n",
        "bars = ax.bar(avg_price.index, avg_price.values, color=\"tab:gray\")\n",
        "\n",
        "ax.set_ylabel(\"Average price (€)\")\n",
        "ax.set_title(\"Average price per discount bucket\")\n",
        "\n",
        "# add labels on top\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x()+bar.get_width()/2, h, f\"€{h:,.0f}\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
