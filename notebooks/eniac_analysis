    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime and numeric\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"total_paid\"] = pd.to_numeric(merged[\"total_paid\"], errors=\"coerce\")\n",
        "\n",
        "# filter zero or negative discounts\n",
        "df_zero_neg = merged[merged[\"discount%\"] <= 0].copy()\n",
        "\n",
        "# revenue = sum of total_paid per order (avoid double-counting)\n",
        "order_revenue = df_zero_neg.groupby(\"order_id\")[\"total_paid\"].first()\n",
        "\n",
        "# attach order date (from merged)\n",
        "order_dates = df_zero_neg.drop_duplicates(\"order_id\").set_index(\"order_id\")[\"created_date\"]\n",
        "\n",
        "# combine\n",
        "rev_df = pd.DataFrame({\"revenue\": order_revenue, \"date\": order_dates})\n",
        "\n",
        "# group by month\n",
        "rev_monthly = rev_df.groupby(rev_df[\"date\"].dt.to_period(\"M\"))[\"revenue\"].sum()\n",
        "rev_monthly.index = rev_monthly.index.to_timestamp()\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(10,5))\n",
        "ax.plot(rev_monthly.index, rev_monthly.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Month\")\n",
        "ax.set_ylabel(\"Revenue (€)\")\n",
        "ax.set_title(\"Revenue of orders with 0 or negative discount\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 507
        },
        "id": "TuPF_CWPZJ1R",
        "outputId": "3fb8b9e0-860d-493f-9a26-622c0090a593"
      },
      "execution_count": null,
