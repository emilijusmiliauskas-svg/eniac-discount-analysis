        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# filter zero discounts\n",
        "zero = merged[merged[\"discount\"] == 0]\n",
        "\n",
        "# zero-discount counts per month\n",
        "counts_zero = zero.groupby(zero[\"created_date\"].dt.to_period(\"M\")).size()\n",
        "\n",
        "# total paid sum per month (only zero discounts)\n",
        "paid_sum_zero = zero.groupby(zero[\"created_date\"].dt.to_period(\"M\"))[\"total_paid\"].sum()\n",
        "\n",
        "# align indexes\n",
        "counts_zero.index = counts_zero.index.to_timestamp()\n",
        "paid_sum_zero.index = paid_sum_zero.index.to_timestamp()\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(10,4))\n",
        "\n",
        "# left axis: counts\n",
        "ax1.plot(counts_zero.index, counts_zero.values, color=\"tab:blue\", label=\"Zero discount count\")\n",
        "ax1.set_xlabel(\"Month\")\n",
        "ax1.set_ylabel(\"Zero discount count\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "\n",
        "# right axis: total paid sum (zero discounts)\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(paid_sum_zero.index, paid_sum_zero.values, color=\"tab:orange\", label=\"Total paid sum (zero discounts)\")\n",
        "ax2.set_ylabel(\"Total paid sum (zero discounts)\", color=\"tab:orange\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "# legends\n",
        "lines1, labels1 = ax1.get_legend_handles_labels()\n",
        "lines2, labels2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(lines1 + lines2, labels1 + labels2, loc=\"upper left\")\n",
        "\n",
        "fig.tight_layout()\n",
        "plt.title(\"Zero discounts: count vs total paid per month\")\n",
        "plt.show()"
