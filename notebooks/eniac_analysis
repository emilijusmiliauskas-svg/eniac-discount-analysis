    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime and numeric\n",
        "merged[\"created_date\"] = pd.to_datetime(merged.get(\"created_date\", merged[\"date\"]), errors=\"coerce\")\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(merged[\"estimated_revenue_per_product\"], errors=\"coerce\")\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "\n",
        "# --- 1) Filter Black Friday window (5 days before and after Nov 24, 2017) ---\n",
        "start = pd.Timestamp(\"2017-11-19\")\n",
        "end   = pd.Timestamp(\"2017-11-29\")\n",
        "bf = merged[(merged[\"created_date\"] >= start) & (merged[\"created_date\"] <= end)].copy()\n",
        "\n",
        "# --- 2) Average discount% in this window ---\n",
        "avg_disc = bf[\"discount%\"].mean()\n",
        "print(f\"Average discount% during BF window: {avg_disc:.2f}%\")\n",
        "\n",
        "# --- 3) ERPP sum by sub_category ---\n",
        "erpp_sub = bf.groupby(\"sub_category\")[\"estimated_revenue_per_product\"].sum().sort_values(ascending=False)\n",
        "erpp_sub.plot(kind=\"bar\", figsize=(10,5), color=\"tab:blue\")\n",
        "plt.title(\"ERPP sum by sub_category (BF window)\")\n",
        "plt.ylabel(\"ERPP sum\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# --- 4) ERPP sum by brand ---\n",
        "erpp_brand = bf.groupby(\"brand\")[\"estimated_revenue_per_product\"].sum().sort_values(ascending=False).head(20)\n",
        "erpp_brand.plot(kind=\"bar\", figsize=(12,5), color=\"tab:green\")\n",
        "plt.title(\"Top 20 brands by ERPP sum (BF window)\")\n",
        "plt.ylabel(\"ERPP sum\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# --- 5) ERPP sum by discount% (10 buckets) ---\n",
        "bf[\"disc_bin\"] = pd.cut(bf[\"discount%\"], bins=10)\n",
        "erpp_disc = bf.groupby(\"disc_bin\")[\"estimated_revenue_per_product\"].sum()\n",
        "erpp_disc.plot(kind=\"bar\", figsize=(10,5), color=\"tab:orange\")\n",
        "plt.title(\"ERPP sum by discount% bins (BF window)\")\n",
        "plt.ylabel(\"ERPP sum\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# --- 6) ERPP sum by price range (10 buckets) ---\n",
        "bf[\"price_bin\"] = pd.cut(bf[\"price\"], bins=10)\n",
        "erpp_price = bf.groupby(\"price_bin\")[\"estimated_revenue_per_product\"].sum()\n",
        "erpp_price.plot(kind=\"bar\", figsize=(10,5), color=\"tab:purple\")\n",
        "plt.title(\"ERPP sum by price range bins (BF window)\")\n",
        "plt.ylabel(\"ERPP sum\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
