: [\n          12.67\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"sub_category\",\n      \"properties\": {\n        \"dtype\": \"category\",\n        \"num_unique_values\": 41,\n        \"samples\": [\n          \"RAM for Mac\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"category\",\n      \"properties\": {\n        \"dtype\": \"category\",\n        \"num_unique_values\": 10,\n        \"samples\": [\n          \"Computers\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"
            }
          },
          "metadata": {},
          "execution_count": 30
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1) keep only orders where total quantity per order_id == 1\n",
        "single_order_ids = (\n",
        "    merged.groupby(\"order_id\")[\"product_quantity\"].sum()\n",
        "    .pipe(lambda s: s[s == 1]).index\n",
        ")\n",
        "df_masked = merged[merged[\"order_id\"].isin(single_order_ids)].copy()\n",
        "\n",
        "# 2) positives only and bucket by discount%\n",
        "pos = df_masked[df_masked[\"discount%\"] > 0].copy()\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "pos[\"disc_range\"] = pd.cut(pos[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "# 3) aggregates per bucket\n",
        "counts = pos.groupby(\"disc_range\").size().reindex(labels, fill_value=0)\n",
        "paid_sum = pos.groupby(\"disc_range\")[\"total_paid\"].sum().reindex(labels, fill_value=0.0)\n",
        "\n",
        "# 4) dual-axis grouped bar chart\n",
        "x = range(len(labels))\n",
        "width = 0.4\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(8,4))\n",
        "bars1 = ax1.bar([i - width/2 for i in x], counts.values, width=width,\n",
        "                color=\"tab:blue\", label=\"Count\")\n",
        "ax1.set_xticks(list(x), labels)\n",
        "ax1.set_ylabel(\"Count\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "\n",
        "ax2 = ax1.twinx()\n",
        "bars2 = ax2.bar([i + width/2 for i in x], paid_sum.values, width=width,\n",
        "                color=\"tab:orange\", label=\"Total paid sum\")\n",
        "ax2.set_ylabel(\"Total paid sum\", color=\"tab:orange\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "# add labels above bars\n",
        "for bar in bars1:\n",
        "    h = bar.get_height()\n",
        "    ax1.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,}\", ha=\"center\", va=\"bottom\", color=\"tab:blue\")\n",
        "\n",
        "for bar in bars2:\n",
        "    h = bar.get_height()\n",
        "    ax2.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\", color=\"tab:orange\")\n",
        "\n",
        "# legend\n",
        "h1, l1 = ax1.get_legend_handles_labels()\n",
        "h2, l2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(h1 + h2, l1 + l2, loc=\"upper left\")\n",
        "\n",
        "plt.title(\"Positive discount buckets (single-item orders): count vs total paid\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
