    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(merged[\"estimated_revenue_per_product\"], errors=\"coerce\")\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "\n",
        "# positives only\n",
        "pos = merged[merged[\"discount%\"] > 0].copy()\n",
        "\n",
        "# buckets\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "pos[\"disc_range\"] = pd.cut(pos[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "# aggregates\n",
        "counts   = pos.groupby(\"disc_range\").size().reindex(labels, fill_value=0)\n",
        "erpp_sum = pos.groupby(\"disc_range\")[\"estimated_revenue_per_product\"].sum().reindex(labels, fill_value=0.0)\n",
        "avg_price = pos.groupby(\"disc_range\")[\"price\"].mean().reindex(labels, fill_value=0.0)\n",
        "\n",
        "# scale avg_price to left axis (counts) for bar height\n",
        "scale = (counts.max() / avg_price.max()) if avg_price.max() > 0 else 1.0\n",
        "avg_price_scaled = avg_price * scale\n",
        "\n",
        "# plotting\n",
        "x = np.arange(len(labels))\n",
        "w = 0.25\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(9,4))\n",
        "\n",
        "# left axis: counts (blue) + scaled avg price (grey)\n",
        "b_count = ax1.bar(x - w, counts.values, width=w, color=\"tab:blue\", label=\"Count\")\n",
        "b_price = ax1.bar(x, avg_price_scaled.values, width=w, color=\"tab:gray\", alpha=0.8, label=\"Avg price (scaled)\")\n",
        "\n",
        "ax1.set_xticks(x, labels)\n",
        "ax1.set_ylabel(\"Count\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "\n",
        "# right axis: ERPP sum (orange)\n",
        "ax2 = ax1.twinx()\n",
        "b_erpp = ax2.bar(x + w, erpp_sum.values, width=w, color=\"tab:orange\", label=\"ERPP sum\")\n",
        "ax2.set_ylabel(\"ERPP sum\", color=\"tab:orange\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "# labels on bars\n",
        "for bar in b_count:\n",
        "    h = bar.get_height()\n",
        "    ax1.text(bar.get_x()+bar.get_width()/2, h, f\"{int(h):,}\", ha=\"center\", va=\"bottom\", color=\"tab:blue\")\n",
        "\n",
        "# print REAL € value inside grey bars\n",
        "for i, bar in enumerate(b_price):\n",
        "    h = bar.get_height()\n",
        "    ax1.text(bar.get_x()+bar.get_width()/2, h/2, f\"€{avg_price.iloc[i]:,.0f}\",\n",
        "             ha=\"center\", va=\"center\", color=\"white\", fontsize=9, fontweight=\"bold\")\n",
        "\n",
        "for bar in b_erpp:\n",
        "    h = bar.get_height()\n",
        "    ax2.text(bar.get_x()+bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\", color=\"tab:orange\")\n",
        "\n",
        "# legend\n",
        "h1,l1 = ax1.get_legend_handles_labels()\n",
        "h2,l2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(h1+h2, l1+l2, loc=\"upper left\")\n",
        "\n",
        "plt.title(\"Positive discount buckets: Count, ERPP sum, and Avg Price (labelled)\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
