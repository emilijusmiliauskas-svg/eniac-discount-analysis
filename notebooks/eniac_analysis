    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# filter positives\n",
        "pos = merged[merged[\"discount\"] > 0]\n",
        "\n",
        "# positive discount counts per month\n",
        "counts_pos = pos.groupby(pos[\"created_date\"].dt.to_period(\"M\")).size()\n",
        "\n",
        "# total paid sum per month (only positive discounts)\n",
        "paid_sum_pos = pos.groupby(pos[\"created_date\"].dt.to_period(\"M\"))[\"total_paid\"].sum()\n",
        "\n",
        "# align indexes\n",
        "counts_pos.index = counts_pos.index.to_timestamp()\n",
        "paid_sum_pos.index = paid_sum_pos.index.to_timestamp()\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(10,4))\n",
        "\n",
        "# left axis: counts\n",
        "ax1.plot(counts_pos.index, counts_pos.values, color=\"tab:green\", label=\"Positive discount count\")\n",
        "ax1.set_xlabel(\"Month\")\n",
        "ax1.set_ylabel(\"Positive discount count\", color=\"tab:green\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:green\")\n",
        "\n",
        "# right axis: total paid sum (positives)\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(paid_sum_pos.index, paid_sum_pos.values, color=\"tab:orange\", label=\"Total paid sum (pos discounts)\")\n",
        "ax2.set_ylabel(\"Total paid sum (pos discounts)\", color=\"tab:orange\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:orange\")\n",
        "\n",
        "# legends\n",
        "lines1, labels1 = ax1.get_legend_handles_labels()\n",
        "lines2, labels2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(lines1 + lines2, labels1 + labels2, loc=\"upper left\")\n",
        "\n",
        "fig.tight_layout()\n",
        "plt.title(\"Positive discounts: count vs total paid per month\")\n",
        "plt.show()"
      ],
