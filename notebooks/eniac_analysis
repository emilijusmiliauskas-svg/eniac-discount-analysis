    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# make sure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(\n",
        "    merged[\"estimated_revenue_per_product\"], errors=\"coerce\"\n",
        ")\n",
        "\n",
        "# total ERPP per brand\n",
        "brand_revenue = merged.groupby(\"brand\")[\"estimated_revenue_per_product\"].sum()\n",
        "\n",
        "# take top 20 brands by revenue\n",
        "top20_brands = brand_revenue.nlargest(20).index\n",
        "\n",
        "# average discount% for those brands\n",
        "avg_disc_top20 = (\n",
        "    merged[merged[\"brand\"].isin(top20_brands)]\n",
        "    .groupby(\"brand\")[\"discount%\"]\n",
        "    .mean()\n",
        "    .sort_values(ascending=False)\n",
        ")\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(12,6))\n",
        "bars = ax.bar(avg_disc_top20.index.astype(str), avg_disc_top20.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Brand\")\n",
        "ax.set_ylabel(\"Average discount %\")\n",
        "ax.set_title(\"Top 20 brands (by ERPP) and their average discount%\")\n",
        "\n",
        "# add labels on top\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:.1f}%\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.xticks(rotation=45, ha=\"right\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
