    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import matplotlib.ticker as mticker\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# weekly grouping\n",
        "per = merged[\"created_date\"].dt.to_period(\"W\")\n",
        "\n",
        "# metrics\n",
        "avg_disc = merged.groupby(per)[\"discount%\"].mean()   # already percent\n",
        "sku_count = merged.groupby(per)[\"sku\"].count()      # total SKU rows (non-unique count)\n",
        "\n",
        "# convert PeriodIndex to Timestamp for plotting\n",
        "x = avg_disc.index.to_timestamp()\n",
        "x2 = sku_count.index.to_timestamp()\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(10,4))\n",
        "\n",
        "# left axis: average discount%\n",
        "ax1.plot(x, avg_disc.values, color=\"tab:blue\", label=\"Avg discount% (weekly)\")\n",
        "ax1.set_xlabel(\"Week\")\n",
        "ax1.set_ylabel(\"Average discount %\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "ax1.yaxis.set_major_formatter(mticker.PercentFormatter(xmax=100))\n",
        "\n",
        "# right axis: sku count (non-unique)\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(x2, sku_count.values, color=\"tab:green\", label=\"Total SKUs sold (weekly)\")\n",
        "ax2.set_ylabel(\"Total SKUs sold\", color=\"tab:green\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:green\")\n",
        "\n",
        "# legend\n",
        "lines1, labels1 = ax1.get_legend_handles_labels()\n",
        "lines2, labels2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(lines1 + lines2, labels1 + labels2, loc=\"upper left\")\n",
        "\n",
        "plt.title(\"Weekly average discount% vs total SKUs sold\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
