    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "# ensure discount% is numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "\n",
        "# define bins and labels\n",
        "bins = [0, 10, 30, float(\"inf\")]\n",
        "labels = [\"0–10%\", \"10–30%\", \">30%\"]\n",
        "\n",
        "# add category column\n",
        "merged[\"disc_range\"] = pd.cut(merged[\"discount%\"], bins=bins, labels=labels, right=True)\n",
        "\n",
        "results = {}\n",
        "\n",
        "for label in labels:\n",
        "    sub = merged[merged[\"disc_range\"] == label]\n",
        "\n",
        "    # descriptive stats for price\n",
        "    desc = sub[\"price\"].describe()\n",
        "\n",
        "    # top 10 names by frequency\n",
        "    top_names = sub[\"name\"].value_counts().head(10)\n",
        "\n",
        "    results[label] = {\n",
        "        \"price_stats\": desc,\n",
        "        \"top_names\": top_names\n",
        "    }\n",
        "\n",
        "# display\n",
        "for label, res in results.items():\n",
        "    print(f\"\\n=== Discount range {label} ===\")\n",
        "    print(\"Price stats:\")\n",
        "    print(res[\"price_stats\"])\n",
        "    print(\"\\nTop 10 product names:\")\n",
        "    print(res[\"top_names\"])"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
