        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# make sure numeric\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(\n",
        "    merged[\"estimated_revenue_per_product\"], errors=\"coerce\"\n",
        ")\n",
        "\n",
        "# aggregate per brand\n",
        "brand_revenue = merged.groupby(\"brand\")[\"estimated_revenue_per_product\"].sum()\n",
        "\n",
        "# top 20\n",
        "top20_brands_erpp = brand_revenue.nlargest(20)\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(12,6))\n",
        "bars = ax.bar(top20_brands_erpp.index.astype(str), top20_brands_erpp.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Brand\")\n",
        "ax.set_ylabel(\"Total estimated revenue per product\")\n",
        "ax.set_title(\"Top 20 brands by total ERPP\")\n",
