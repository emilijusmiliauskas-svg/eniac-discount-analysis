    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# weekly grouping\n",
        "per = merged[\"created_date\"].dt.to_period(\"W\")\n",
        "\n",
        "# metric: total paid sum per week\n",
        "paid_sum = merged.groupby(per)[\"total_paid\"].sum()\n",
        "\n",
        "# convert PeriodIndex to Timestamp for plotting\n",
        "x = paid_sum.index.to_timestamp()\n",
        "\n",
        "plt.figure(figsize=(10,4))\n",
        "plt.plot(x, paid_sum.values, color=\"tab:orange\")  # no markers\n",
        "plt.xlabel(\"Week\")\n",
        "plt.ylabel(\"Total paid sum\")\n",
        "plt.title(\"Weekly total paid sum (Revenue)\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 407
        },
        "id": "TxFqWebDN4A6",
