    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import matplotlib.ticker as mticker\n",
        "\n",
        "# ensure datetime\n",
        "merged[\"created_date\"] = pd.to_datetime(merged[\"date\"], errors=\"coerce\")\n",
        "\n",
        "# quarterly grouping\n",
        "per = merged[\"created_date\"].dt.to_period(\"Q\")\n",
        "\n",
        "# metrics\n",
        "avg_disc = merged.groupby(per)[\"discount%\"].mean()   # %\n",
        "sku_count = merged.groupby(per)[\"sku\"].count()\n",
        "\n",
        "# convert PeriodIndex to Timestamp for plotting\n",
        "x = avg_disc.index.to_timestamp()\n",
        "x2 = sku_count.index.to_timestamp()\n",
        "\n",
        "# build custom labels like \"Jan–Mar 2022\"\n",
        "labels = [f\"{p.start_time.strftime('%b')}–{p.end_time.strftime('%b %Y')}\" for p in avg_disc.index]\n",
        "\n",
        "fig, ax1 = plt.subplots(figsize=(12,5))\n",
        "\n",
        "# left axis: avg discount%\n",
        "ax1.plot(x, avg_disc.values, color=\"tab:blue\", marker=\"o\", label=\"Avg discount% (quarterly)\")\n",
        "ax1.set_xlabel(\"Quarter\")\n",
        "ax1.set_ylabel(\"Average discount %\", color=\"tab:blue\")\n",
        "ax1.tick_params(axis=\"y\", labelcolor=\"tab:blue\")\n",
        "ax1.yaxis.set_major_formatter(mticker.PercentFormatter(xmax=100))\n",
        "\n",
        "# right axis: sku count\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(x2, sku_count.values, color=\"tab:green\", marker=\"s\", label=\"Total SKUs sold (quarterly)\")\n",
        "ax2.set_ylabel(\"Total SKUs sold\", color=\"tab:green\")\n",
        "ax2.tick_params(axis=\"y\", labelcolor=\"tab:green\")\n",
        "\n",
        "# custom x-ticks\n",
        "ax1.set_xticks(x)\n",
        "ax1.set_xticklabels(labels, rotation=45, ha=\"right\")\n",
        "\n",
        "# legend\n",
        "lines1, labels1 = ax1.get_legend_handles_labels()\n",
        "lines2, labels2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(lines1 + lines2, labels1 + labels2, loc=\"upper left\")\n",
        "\n",
        "plt.title(\"Quarterly average discount% vs total SKUs sold\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 503
        },
        "id": "3IK2Ghk2NWUZ",
        "outputId": "4b2abd5d-0e10-4712-cb21-ddbe57f62d2e"
      },
