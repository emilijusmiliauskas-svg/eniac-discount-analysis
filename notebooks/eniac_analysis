      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"estimated_revenue_per_product\"] = pd.to_numeric(\n",
        "    merged[\"estimated_revenue_per_product\"], errors=\"coerce\"\n",
        ")\n",
        "\n",
        "# filter Apple only\n",
        "apple_df = merged[merged[\"brand\"].str.lower() == \"apple\"]\n",
        "\n",
        "# group by category, sum ERPP\n",
        "apple_erpp = (\n",
        "    apple_df.groupby(\"category\")[\"estimated_revenue_per_product\"]\n",
        "    .sum()\n",
        "    .sort_values(ascending=False)\n",
        ")\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(10,5))\n",
        "bars = ax.bar(apple_erpp.index.astype(str), apple_erpp.values, color=\"tab:blue\")\n",
        "\n",
        "ax.set_xlabel(\"Category\")\n",
        "ax.set_ylabel(\"Total ERPP\")\n",
        "ax.set_title(\"Apple revenue by category (ERPP)\")\n",
        "\n",
        "# labels on top\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.xticks(rotation=45, ha=\"right\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
