    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"total_paid\"] = pd.to_numeric(merged[\"total_paid\"], errors=\"coerce\")\n",
        "\n",
        "# classify discount type\n",
        "merged[\"disc_type\"] = merged[\"discount%\"].apply(lambda x: \"Zero\" if x == 0 else (\"Negative\" if x < 0 else \"Other\"))\n",
        "\n",
        "# keep only Zero and Negative\n",
        "df_sel = merged[merged[\"disc_type\"].isin([\"Zero\",\"Negative\"])]\n",
        "\n",
        "# revenue per order (avoid double-counting)\n",
        "order_rev = df_sel.groupby(\"order_id\")[\"total_paid\"].first().reset_index()\n",
        "order_type = df_sel.drop_duplicates(\"order_id\")[[\"order_id\",\"disc_type\"]]\n",
        "\n",
        "rev_df = pd.merge(order_rev, order_type, on=\"order_id\")\n",
        "\n",
        "# aggregate by discount type\n",
        "rev_sum = rev_df.groupby(\"disc_type\")[\"total_paid\"].sum()\n",
        "\n",
        "# plot\n",
        "fig, ax = plt.subplots(figsize=(6,4))\n",
        "bars = ax.bar(rev_sum.index, rev_sum.values, color=[\"tab:blue\",\"tab:orange\"])\n",
        "\n",
        "ax.set_ylabel(\"Revenue (€)\")\n",
        "ax.set_title(\"Revenue by discount type (0% vs Negative)\")\n",
        "\n",
        "# labels on bars\n",
        "for bar in bars:\n",
        "    h = bar.get_height()\n",
        "    ax.text(bar.get_x() + bar.get_width()/2, h, f\"{h:,.0f}\", ha=\"center\", va=\"bottom\")\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 407
        },
        "id": "qFA-IlcUZr2q",
        "outputId": "d1b22388-c0a9-4bf3-ba21-5cf1cd2be509"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 600x400 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"product_quantity\"] = pd.to_numeric(merged[\"product_quantity\"], errors=\"coerce\")\n",
        "\n",
        "# classify discount type\n",
        "merged[\"disc_type\"] = merged[\"discount%\"].apply(\n",
        "    lambda x: \"Positive\" if x > 0 else (\"Negative\" if x < 0 else \"Zero\")\n",
        ")\n",
        "\n",
        "# total quantity per order\n",
        "qty_per_order = merged.groupby(\"order_id\")[\"product_quantity\"].sum()\n",
        "\n",
        "# attach order size to each row\n",
        "merged = merged.join(qty_per_order.rename(\"order_qty\"), on=\"order_id\")\n",
        "\n",
        "# count discount types per order size\n",
        "counts = (\n",
        "    merged.groupby([\"order_qty\",\"disc_type\"])[\"order_id\"]\n",
        "    .nunique()   # count orders, not rows\n",
        "    .unstack(fill_value=0)\n",
        ")\n",
        "\n",
        "# plot stacked bars\n",
        "counts.plot(\n",
        "    kind=\"bar\",\n",
        "    stacked=True,\n",
        "    figsize=(10,6),\n",
        "    color={\"Positive\":\"tab:green\",\"Zero\":\"tab:gray\",\"Negative\":\"tab:red\"}\n",
        ")\n",
        "\n",
        "plt.xlabel(\"Order total quantity\")\n",
        "plt.ylabel(\"Number of orders\")\n",
        "plt.title(\"Orders by size and discount type\")\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
