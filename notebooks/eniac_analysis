    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ensure numeric\n",
        "merged[\"discount%\"] = pd.to_numeric(merged[\"discount%\"], errors=\"coerce\")\n",
        "merged[\"price\"] = pd.to_numeric(merged[\"price\"], errors=\"coerce\")\n",
        "\n",
        "# bin discounts into 20 buckets\n",
        "bins = 20\n",
        "merged[\"disc_bin\"] = pd.cut(merged[\"discount%\"], bins=bins)\n",
        "\n",
        "# prepare data: price ranges per discount% bin\n",
        "price_data = [merged.loc[merged[\"disc_bin\"] == b, \"price\"].dropna() for b in merged[\"disc_bin\"].cat.categories]\n",
        "\n",
        "# plot as boxplot\n",
        "plt.figure(figsize=(12,6))\n",
        "plt.boxplot(price_data, labels=[str(c) for c in merged[\"disc_bin\"].cat.categories], showfliers=False)\n",
        "\n",
        "plt.xlabel(\"Discount% range\")\n",
